{
  "name": "arcane-chess",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run build:server && npm run build:frontend && npm run build:css",
    "build:frontend": "vite build --outDir dist/frontend && tsc -p tsconfig.json",
    "build:server": "tsc -p tsconfig.server.json",
    "build:css": "sass src/styles/main.scss dist/frontend/main.css",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "redis": "docker start redis",
    "server": "nodemon --exec ts-node --project tsconfig.server.json server/server.ts",
    "backend": "npm run redis && npm run server",
    "start": "node dist/server/server.js",
    "heroku-postbuild": "npm run build"
  },
