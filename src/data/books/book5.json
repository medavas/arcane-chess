    "title": "Preparation and Resources",
    "nodeText": "",
    "storyTitle": "",
    "storyText": "",
    "hero": "",
    "opponent": "",
    "theme": "everwanderers",
    "bookTheme": "everwanderers",
    "time": [
      [0, 0],
