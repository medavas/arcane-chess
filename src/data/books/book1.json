        "blackArcane": {},
        "config": {},
        "correctMoves": [],
        "viewOnly": true,
        "orientation": "white"
      },
      "panel-2": {
        "fen": "8/8/8/8/8/8/8/8 w - - 0 1",
        "fenHistory": ["8/8/8/8/8/8/8/8 w - - 0 1"],
        "history": [],
        "panelText": "Prompts like these will help you search for meaning in the position, organize your thinking, and hydrate your plans. We’ll touch on plans and more helpful questions later.",
        "arrowsCircles": [],
        "royalties": {
          "royaltyQ": {},
          "royaltyT": {},
          "royaltyM": {},
          "royaltyV": {},
