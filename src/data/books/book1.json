          "royaltyT": {},
          "royaltyM": {},
          "royaltyV": {},
          "royaltyE": {}
        },
        "preset": "CLEAR",
        "whiteArcane": {},
        "blackArcane": {},
        "config": {},
        "correctMoves": [],
        "viewOnly": false,
        "orientation": "white"
      },
      "panel-6": {
        "fen": "2u5/8/4u1z1/4N3/2n3N1/3U1Z2/8/8 w - - 0 1",
        "fenHistory": ["2u5/8/4u1z1/4N3/2n3N1/3U1Z2/8/8 w - - 0 1"],
        "history": [],
        "panelText": "You’ll have to spend some time with these pieces and understand their tactics, don’t underestimate them in closed positions!",
