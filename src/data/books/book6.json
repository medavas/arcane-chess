    "reward": [],
    "panels": {
      "panel-1": {
        "fen": "8/8/8/8/8/8/8/8 w - - 0 1",
        "fenHistory": ["8/8/8/8/8/8/8/8 w - - 0 1"],
        "history": [],
        "panelText": "Drive in chess is the internal force that propels players to keep improving, striving for mastery, and staying committed to their goals. It’s what fuels long hours of studying openings, practicing endgames, and analyzing games to find even the smallest areas for improvement. In life, drive serves as the compass that keeps you moving toward your long-term aspirations. Just as a chess player is motivated by the pursuit of greatness, life’s achievements often stem from an unwavering desire to grow and succeed.",
        "arrowsCircles": [],
        "nodeText": "",
        "reward": [],
        "description": "",
        "opponent": "",
        "prereq": "",
