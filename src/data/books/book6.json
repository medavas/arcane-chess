  "lesson-2": {
    "id": "lesson-2",
    "prereq": "lesson-1",
    "title": "Drive, Persistence",
    "nodeText": "",
    "storyTitle": "",
    "storyText": "",
    "hero": "",
    "opponent": "",
    "theme": "",
    "bookTheme": "",
    "time": [
      [0, 0],
