    "theme": "",
    "panels": {
      "panel-1": {
        "fen": "8/8/8/8/8/8/8/8 w - - 0 1",
        "fenHistory": ["8/8/8/8/8/8/8/8 w - - 0 1"],
        "history": [],
        "panelText": "Space is a built in mechanic of the chessboard. Each of the 64 squares represents a piece of it. That you want to control. The more space you have the better your chance of converting to an endgame with a Pawn that is fast to promote. Army to begin the game is confined to this close-quarter zone, each cornered against a hypothetical wall.",
        "arrowsCircles": [],
        "nodeText": "",
        "reward": [],
        "description": "",
        "opponent": "",
        "prereq": "",
        "royalties": {
          "royaltyQ": {},
          "royaltyT": {},
          "royaltyM": {},
          "royaltyV": {},
          "royaltyE": {}
